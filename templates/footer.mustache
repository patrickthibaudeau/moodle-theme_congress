{{!
    @template theme_boost/footer

    Page footer.

    Example context (json):
    {
        "output": {
            "page_doc_link": "Help and documentation",
            "supportemail": "<a href=\"#\">Contact site support</a>",
            "has_popover_links": true,
            "services_support": "Services and support",
            "login_info": "You are logged in as cute kitten",
            "moodle_release": "90210"
        }
    }
}}

<footer id="page-footer" class="main-footer">
    <div class="footer-section p-3 border-bottom">
        <div class="logininfo">
            {{{ output.login_info }}}
        </div>
        <div class="tool_usertours-resettourcontainer">
        </div>

        {{{ output.standard_footer_html }}}

    </div>
    <div class="footer-content-debugging footer-dark bg-dark text-light">
        <div class="container-fluid footer-dark-inner">
            {{{ output.debug_footer_html }}}
        </div>
    </div>
</footer>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
    $.widget.bridge('uibutton', $.ui.button)
</script>
<!-- Congress Default-->
<script src="{{config.wwwroot}}/theme/congress/js/default.js"></script>
<!-- ChartJS -->
<script src="{{config.wwwroot}}/theme/congress/plugins/chart.js/Chart.min.js"></script>
<!-- Sparkline -->
<script src="{{config.wwwroot}}/theme/congress/plugins/sparklines/sparkline.js"></script>
<!-- JQVMap -->
<script src="{{config.wwwroot}}/theme/congress/plugins/jqvmap/jquery.vmap.min.js"></script>
<script src="{{config.wwwroot}}/theme/congress/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
<!-- jQuery Knob Chart -->
<script src="{{config.wwwroot}}/theme/congress/plugins/jquery-knob/jquery.knob.min.js"></script>
<!-- daterangepicker -->
<script src="{{config.wwwroot}}/theme/congress/plugins/moment/moment.min.js"></script>
<script src="{{config.wwwroot}}/theme/congress/plugins/daterangepicker/daterangepicker.js"></script>
<!-- Tempusdominus Bootstrap 4 -->
<script src="{{config.wwwroot}}/theme/congress/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
<!-- Summernote -->
<script src="{{config.wwwroot}}/theme/congress/plugins/summernote/summernote-bs4.min.js"></script>
<!-- overlayScrollbars -->
<script src="{{config.wwwroot}}/theme/congress/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
<!-- AdminLTE App -->
<script src="{{config.wwwroot}}/theme/congress/js/adminlte.min.js"></script>
<!-- DataTables -->
<script type="text/javascript" src="{{config.wwwroot}}/theme/congress/js/DataTables/datatables.min.js"></script>
<!-- Select2 -->
<script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@ttskch/select2-bootstrap4-theme@x.x.x/dist/select2-bootstrap4.min.css">

{{{ output.standard_end_of_body_html }}}
